<Robot type="FirstInspires-FTC" name="Latest" >
    <!-- See this for explanation https://ftc9929.com/2019/12/16/stress-free-ftc-hardware-configurations/  -->
    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <goBILDA5202SeriesMotor name="frontLeftMotor" port="0" />  <!-- Put wire color here -->
            <goBILDA5202SeriesMotor name="backLeftMotor" port="1" /> <!-- Put wire color here -->
            <goBILDA5202SeriesMotor name="frontRightMotor" port="2" /> <!-- Put wire color here -->
            <goBILDA5202SeriesMotor name="backRightMotor" port="3" /> <!-- Put wire color here -->
            <Servo name="clawServo" port="0" /> <!-- Put wire color here -->
            <Servo name="armServo" port="1" /> <!-- Put wire color here -->

            <LynxEmbeddedIMU name="imu" port="0" bus="0" />
        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">
            <goBILDA5202SeriesMotor name="armMotor" port="0" /> <!-- Put wire color here -->
            <goBILDA5202SeriesMotor name="armMotor2" port="1" /> <!-- Put wire color here -->
        </LynxModule>
    </LynxUsbDevice>
</Robot>